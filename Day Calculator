            else if (d - 0 == 6 || d - 7 == 6 || d - 14 == 6 || d - 21 == 6 || d - 28 == 6)
            {
                cout << "SUNDAY";
            }
            else if (d - 0 == 7 || d - 7 == 7 || d - 14 == 7 || d - 21 == 7 || d - 28 == 7)
            {
                cout << "MONDAY";
            }
            else if (d - 0 == 1 || d - 7 == 1 || d - 14 == 1 || d - 21 == 1 || d - 28 == 1)
            {
                cout << "TUESDAY";
            }
            else if (d - 0 == 2 || d - 7 == 2 || d - 14 == 2 || d - 21 == 2 || d - 28 == 2)
            {
                cout << "WEDNESDAY";
            }
        }

        // OCTOBER
        else if (m == 10 && d >= 1 && d <= 31)
        {

            if (d - 0 == 2 || d - 7 == 2 || d - 14 == 2 || d - 21 == 2 || d - 28 == 2)
            {
                cout << "SATURDAY";
            }
            else if (d - 0 == 3 || d - 7 == 3 || d - 14 == 3 || d - 21 == 3 || d - 28 == 3)
            {
                cout << "SUNDAY";
            }
            else if (d - 0 == 4 || d - 7 == 4 || d - 14 == 4 || d - 21 == 4 || d - 28 == 4)
            {
                cout << "MONDAY";
            }
            else if (d - 0 == 5 || d - 7 == 5 || d - 14 == 5 || d - 21 == 5 || d - 28 == 5)
            {
                cout << "TUESDAY";
            }
            else if (d - 0 == 6 || d - 7 == 6 || d - 14 == 6 || d - 21 == 6 || d - 28 == 6)
            {
                cout << "WEDNESDAY";
            }
            else if (d - 0 == 7 || d - 7 == 7 || d - 14 == 7 || d - 21 == 7 || d - 28 == 7)
            {
                cout << "THURSDAY";
            }
            else if (d - 0 == 1 || d - 7 == 1 || d - 14 == 1 || d - 21 == 1 || d - 28 == 1)
            {
                cout << "FRIDAY";
            }
        }

        // September and December
        else if (m == 9 || m == 12 && d >= 1 && d <= 31)
        {

            if (d - 0 == 2 || d - 7 == 2 || d - 14 == 2 || d - 21 == 2 || d - 28 == 2)
            {
                cout << "THURSDAY";
            }
            else if (d - 0 == 3 || d - 7 == 3 || d - 14 == 3 || d - 21 == 3 || d - 28 == 3)
            {
                cout << "FRIDAY";
            }
            else if (d - 0 == 4 || d - 7 == 4 || d - 14 == 4 || d - 21 == 4 || d - 28 == 4)
            {
                cout << "SATURDAY";
            }
            else if (d - 0 == 5 || d - 7 == 5 || d - 14 == 5 || d - 21 == 5 || d - 28 == 5)
            {
                cout << "SUNDAY";
            }
            else if (d - 0 == 6 || d - 7 == 6 || d - 14 == 6 || d - 21 == 6 || d - 28 == 6)
            {
                cout << "MONDAY";
            }
            else if (d - 0 == 7 || d - 7 == 7 || d - 14 == 7 || d - 21 == 7 || d - 28 == 7)
            {
                cout << "TUESDAY";
            }
            else if (d - 0 == 1 || d - 7 == 1 || d - 14 == 1 || d - 21 == 1 || d - 28 == 1)
            {
                cout << "WEDNESDAY";
            }
        }
    }

    else
    {
        cout << "Wrong Entry";
    }

    return 0;
}
